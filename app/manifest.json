{
  "name": "__MSG_appName__",
  "version": "0.0.7",
  "manifest_version": 3,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/notebook-16.png",
    "64": "images/notebook-64.png",
    "128": "images/notebook-128.png"
  },
  "default_locale": "en",
  "action": {
    "default_icon": "images/notebook-128.png",
    "default_title": "My Secret Notes"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs",
    "notifications"
  ]
}
